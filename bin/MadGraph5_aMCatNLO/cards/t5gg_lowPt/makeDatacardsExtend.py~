NLSP=["100","300","500","700","1000"]

delmap ={
    "100":["90","70"],
    "300":["290","270","200"],
    "500":["490","470","400"],
    "700":["690","670","600"],
    "1000":["990","970","900"]




}
#g2200_X100_delm30

for n in NLSP:
    #print n
    for delm in delmap[n]:
        #print n, delm
        #print "================="
        nstring=n
        delm_string=delm
        diff  = int(n) -int(delm)
        print diff
        diff_string=str(diff)
        #print "========="
        fDir= "g2200_X"+nstring+"_delm"+diff_string+"/"
        print fDir
        if not os.path.exists(fDir):
            mk= "mkdir "+fDir
            print "bleep"
            os.system(mk)
            print "bloop"
            fname1="g2200_X"+nstring+"_delm"+diff_string+"_param_card.dat"
            fname2="g2200_X"+nstring+"_delm"+diff_string+"_run_card.dat"
            fname3="g2200_X"+nstring+"_delm"+diff_string+"_proc_card.dat"
            #print fname1,fname2,fname3
